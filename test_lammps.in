units metal
atom_style atomic
atom_modify map array sort 0 0.0
boundary p p p
lattice bcc 3.144221296574379 orient x 1 1 1 orient y -1 2 -1 orient z -1 0 1
region r_simbox block -1e-9 10.0000000001 -1e-9 10.0000000001 -5.0000000001 15.0000000001 units lattice
region r_atombox block -1e-4 10.0001 -1e-4 10.0001 -1e-4 10.0001 units lattice
create_box 3 r_simbox
